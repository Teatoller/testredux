{
  "name": "testredux",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.0",
    "react-test-renderer": "^17.0.2",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0",
    "web-vitals": "^0.2.4",
    "whatwg-fetch": "^3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "coverage": "yarn test -- --coverage  --watchAll=false && cat ./coverage/lcov.info | coveralls",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageReporters": [
      "json",
      "text",
      "html",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/**/store.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!<rootDir>/node_modules/",
      "!**/templates/**",
      "!**/template/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 15,
        "lines": 15,
        "statements": 15
      }
    }
  }
}